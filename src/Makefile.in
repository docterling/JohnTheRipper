default:
	$(MAKE) find_version
	@$(MAKE) $(PROJ) \
		JOHN_OBJS="$(JOHN_OBJS) @CC_ASM_OBJS@"

	@if [ "$(OS)" = "Windows_NT" ]; then \
		$(MAKE) peflags; \
	fi
